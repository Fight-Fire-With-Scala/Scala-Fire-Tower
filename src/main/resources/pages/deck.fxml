<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id="deckPane" styleClass="sidebarPane" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <StackPane style="-fx-padding: 10 31 10 31;">
        <VBox alignment="CENTER" spacing="5" prefHeight="80.0" prefWidth="138.0">
            <Button fx:id="discardButton" prefWidth="121.0" text="Discard Card"/>
            <HBox alignment="CENTER" spacing="5">
                <Button fx:id="okButton" text="OK"/>
                <Button fx:id="cancelButton" text="Cancel"/>
            </HBox>
        </VBox>
    </StackPane>
</StackPane>